<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Test">
	<insert id="Test.insert1" parameterType="TestDto">
	insert into test1
	( id, test)
	values(#{id}, #{test})
	</insert>
	
	<insert id="Test.insert2" parameterType="TestDto">
	insert into test2
	( id, test)
	values(#{id}, #{test})
	</insert>
</mapper>
